!!
!! .Xresources
!!

!! Appearance
*background: black
*foreground: #CCCCCC
*scrollBar: false
URxvt*transparent: true
URxvt*inheritPixmap: true
URxvt*shading: 20
!!URxvt*borderLess: true
URxvt*geometry: 126x132

!! Fonts
URxvt*font: xft:Terminess\ Powerline:pixelsize=14:antialias=true
URxvt*boldFont: xft:Terminess\ Powerline:pixelsize=14:antialias=true
Xft.autohint: true
Xft.antialias: true
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

!! Clickable URLs
URxvt.perl-ext-common: default,matcher
!!,tabbed
!!URxvt.url-launcher: chromium
!!URxvt.url-launcher: google-chrome
!!URxvt.urlLauncher: midori
URxvt.url-launcher: firefox
URxvt.matcher.button: 1
URxvt.matcher.pattern.1: \\bwww\\.[\\w-]\\.[\\w./?&@!-]*[\\w/-]

!! Tabs
URxvt.tabbed.tabbar-fg: 8
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tab-fg: 2
URxvt.tabbed.tab-bg: -1

!! Keybindings
URxvt*eightBitInput:   false
URxvt*metaSendsEscape: true
XTerm*eightBitInput:   false
XTerm*metaSendsEscape: true

!! Scroll behavior
URxvt.searchable-scrollback: CM-s
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true
URxvt*saveLines: 10000

! black
*.color0: #2E2E34
*.color8: #555555
! ! red
*.color1: #FF005C
*.color9: #FF5995
! ! green
*.color2: #82B414
*.color10: #A9EE29
! ! yellow
*.color3: #FD971F
*.color11: #FEED6C
! ! blue
*.color4: #0080FF
*.color12: #65B2FF
! ! magenta
*.color5: #8C54FE
*.color13: #B691FF
! ! cyan
*.color6: #00B08F
*.color14: #76E1EF
! ! white
*.color7: #CCCCCC
*.color15: #FFFFFF
